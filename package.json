{
  "name": "tandoori-ui",
  "version": "1.0.0",
  "description": "Tandoori user interface's elements.",
  "main": "dist/js/index.js",
  "module": "dist/js/index.js",
  "jsnext:main": "dist/js/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "buil:js": "webpack",
    "build-dev:js": "webpack --watch",
    "build-dev:webpack-dashboard": "webpack-dashboard -- webpack --watch",
    "build:sass": "sass static/sass/main.scss:public/css/tandoori-ui.css",
    "build-dev:sass": "sass --watch static/sass/main.scss:public/css/tandoori-ui.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Spicesoft/tandoori-ui.git"
  },
  "author": "dev@tandoori.pro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Spicesoft/tandoori-ui/issues"
  },
  "homepage": "https://spicesoft.github.io/tandoori-ui/",
  "sasslintConfig": "./.sass-lint.yml",
  "devDependencies": {
    "@storybook/react": "^3.1.3",
    "babel": "^6.23.0",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap-sass": "^3.3.7",
    "enzyme": "^2.9.1",
    "eslint": "3.12.2",
    "eslint-plugin-html": "1.7.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsdoc": "2.4.0",
    "eslint-plugin-react": "6.8.0",
    "eslint-plugin-requirejs": "3.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "font-awesome": "^4.7.0",
    "jest": "^20.0.4",
    "node-sass": "^4.5.3",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.6.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "stylelint": "^7.10.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "dist/js/navbar/**/*.{js,jsx}",
      "static/js/side-menu/**/*.{js,jsx}",
      "dist/js/shared/**/*.{js,jsx}"
    ],
    "coverageDirectory": "static/js/coverage/",
    "coverageReporters": [
      "html",
      "json",
      "lcov"
    ],
    "coveragePathIgnorePatterns": [
      "App.jsx"
    ]
  }
}
